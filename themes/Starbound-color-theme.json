{
	"name": "Starbound",
	"colors": {
		"editor.background": "#000000ff",
		"editor.foreground": "#d8d1cc",
		"sideBar.background": "#12101cff",
		"sideBar.foreground": "#fff5ee"
	},
	"tokenColors": [
		{
			"name": "Code - Comments",
			"scope": [
				"comment",
				"string.quoted.docstring.multi.python"
			],
			"settings": {
				"foreground": "#a59f99",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Code - Keywords",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#955cff"
			}
		},
		{
			"name": "Code - Functions",
			"scope": [
				"entity.name.function",
				"meta.function-call.generic"
			],
			"settings": {
				"foreground": "#52d9ff"
			}
		},
		{
			"name": "Code - Function Calls",
			"scope": [
				"variable.parameter.function-call"
			],
			"settings": {
				"foreground": "#ea9300ff"
			}
		},
		{
			"name": "Code - Classes",
			"scope": [
				"entity.name.type"
			],
			"settings": {
				"foreground": "#ea9300ff"
			}
		},
		{
			"name": "Code - Numbers",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#ea9300ff"
			}
		},
		{
			"name": "Code - Strings",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#e8e200ff"
			}
		},
		{
			"name": "Code - Special",
			"scope": [
				"variable.language",
				"constant.language",
				"constant.other"
			],
			"settings": {
				"foreground": "#ea3900ff"
			}
		},
		{
			"name": "Markdown - Headers",
			"scope": [
				"markup.heading.setext",
				"punctuation.definition.heading.markdown"
			],
			"settings": {
				"foreground": "#955cff"
			}
		},
		{
			"name": "Markdown - Table",
			"scope": [
				"punctuation.definition.table",
				"punctuation.separator.table"
			],
			"settings": {
				"foreground": "#ea9300ff"
			}
		},
		{
			"name": "Markdown - Link Title",
			"scope": [
				"string.other.link.description",
				"string.other.link.title",
			],
			"settings": {
				"foreground": "#e8e200ff"
			}
		},
		{
			"name": "Markdown - Link Link",
			"scope": [
				"markup.underline.link.markdown",
				"markup.underline.link.image.markdown"
			],
			"settings": {
				"foreground": "#52d9ff",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markdown - Link Punctuation",
			"scope": [
				"punctuation.definition.link",
				"punctuation.definition.metadata"
			],
			"settings": {
				"foreground": "#955cff"
			}
		},
		{
			"name": "Markdown - Code",
			"scope": [
				"markup.raw.block.markdown",
				"markup.inline.raw.string.markdown",
				"fenced_code.block.language.markdown"
			],
			"settings": {
				"foreground": "#52d9ff"
			}
		},
		{
			"name": "Markdown - List",
			"scope": [
				"punctuation.definition.list"
			],
			"settings": {
				"foreground": "#52d9ff",
			}
		},
		{
			"name": "Markdown - Punctuation - Other",
			"scope": [
				"punctuation.definition.raw",
				"markup.fenced_code.block.markdown"
			],
			"settings": {
				"foreground": "#ea9300ff"
			}
		},
		{
			"name": "Markup - Key",
			"scope": [
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#52d9ff"
			}
		},
		{
			"name": "Markup - Property Name",
			"scope": [
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#ea9300ff"
			}
		},
		{
			"name": "Markup - Attribute",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#ea9300ff"
			}
		},
		{
			"name": "CSS - Entity",
			"scope": [
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.id.css"
			],
			"settings": {
				"foreground": "#955cff",
			}
		},
		{
			"name": "CSS - Name",
			"scope": [
				"support.type.property-name.css",
				"entity.name.tag.reference.scss"
			],
			"settings": {
				"foreground": "#d8d1cc"
			}
		},
		{
			"name": "CSS - Colors",
			"scope": [
				"constant.other.color",
				"support.constant.color"
			],
			"settings": {
				"foreground": "#ea9300ff"
			}
		},
		{
			"name": "CSS - Operations",
			"scope": [
				"support.function.misc.scss",
				"keyword.control.at-rule.extend.scss",
				"keyword.control.at-rule.import.scss",
				"keyword.control.for.scss"
			],
			"settings": {
				"foreground": "#52d9ff"
			}
		},
		{
			"name": "CSS - Constant",
			"scope": [
				"support.constant.property-value.css"
			],
			"settings": {
				"foreground": "#ea3900ff"
			}
		},
		{
			"name": "CSS - Numeric",
			"scope": [
				"keyword.other.unit.percentage.css",
				"keyword.other.unit.pt.css",
				"keyword.other.unit.px.css",
				"keyword.other.unit.em.css",
				"keyword.other.unit.vw.css",
				"keyword.other.unit.vh.css",
				"keyword.other.unit.s.css"
			],
			"settings": {
				"foreground": "#ea9300ff"
			}
		}
	]
}
